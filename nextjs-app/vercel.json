{
  "version": 2,
  "name": "clean-racing-league",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "app/api/standings/route.ts": {
      "maxDuration": 30
    },
    "app/api/race-results/route.ts": {
      "maxDuration": 30
    }
  }
}